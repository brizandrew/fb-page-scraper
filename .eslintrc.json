{
    "rules": {
        "indent": [2, 4, {
          "SwitchCase": 1
        }],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-console": 0,
        "no-unused-vars": 1,
        "dot-notation": 2
    },
    "globals": {
      "window": true,
      "jQuery": true,
      "$": true,
      "document": true,
      "fetch": true
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 6
    },
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "plugins": [
    ]
}
